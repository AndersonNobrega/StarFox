<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="350.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.login.LoginController">
   <children>
      <Label layoutX="50.0" layoutY="100.0" text="Username" />
      <Label layoutX="50.0" layoutY="150.0" text="Password" />
      <PasswordField fx:id="password" layoutX="185.0" layoutY="145.0" prefHeight="25.0" prefWidth="230.0" />
      <TextField fx:id="username" layoutX="185.0" layoutY="95.0" onAction="#login" prefHeight="25.0" prefWidth="230.0" />
      <Button fx:id="loginButton" layoutX="185.0" layoutY="205.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Button fx:id="signUpButton" layoutX="345.0" layoutY="205.0" mnemonicParsing="false" onAction="#signUp" text="Sign Up" />
      <Label layoutX="47.0" layoutY="30.0" text="Alpha Version" />
      <Label fx:id="loginStatus" layoutX="42.0" layoutY="204.0" prefHeight="25.0" prefWidth="101.0" />
   </children>
</AnchorPane>
