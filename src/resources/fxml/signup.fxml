<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="350.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.signup.SignUpController">
    <children>
        <Label layoutX="50.0" layoutY="50.0" text="Username" />
        <Label layoutX="50.0" layoutY="100.0" text="Password" />
        <PasswordField fx:id="firstPassword" layoutX="185.0" layoutY="95.0" prefHeight="25.0" prefWidth="230.0" />
        <TextField fx:id="username" layoutX="185.0" layoutY="45.0" prefHeight="25.0" prefWidth="230.0" />
        <Button fx:id="createAccount" layoutX="296.0" layoutY="215.0" mnemonicParsing="false" onAction="#creatingAccount" text="Create Account" />
        <Label fx:id="accountStatus" layoutX="185.0" layoutY="280.0" prefHeight="25.0" prefWidth="235.0" />
      <Label layoutX="50.0" layoutY="150.0" text="Re-enter Password" />
      <PasswordField fx:id="secondPassword" layoutX="185.0" layoutY="145.0" prefHeight="25.0" prefWidth="230.0" />
      <Button fx:id="back" layoutX="185.0" layoutY="215.0" mnemonicParsing="false" onAction="#backButton" text="Back" />
    </children>
</AnchorPane>
